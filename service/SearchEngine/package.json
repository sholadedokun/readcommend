{
  "name": "searchengine",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon index.js",
    "dev": "nodemon --inspect-brk index.js",
    "test": "jest --runInBand --testTimeout=10000",
    "test:watch": "npm run pretest && node --inspect-brk node_modules/.bin/jest --runInBand --watch",
    "test:watch:coverage": "npm run pretest && node --inspect-brk node_modules/.bin/jest --runInBand --watch --coverage",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nodemon": "^2.0.21",
    "pg": "^8.10.0",
    "sequelize": "^6.29.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@faker-js/faker": "^7.6.0",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "sequelize-cli": "^6.6.0",
    "sinon": "^15.0.2"
  }  
}
